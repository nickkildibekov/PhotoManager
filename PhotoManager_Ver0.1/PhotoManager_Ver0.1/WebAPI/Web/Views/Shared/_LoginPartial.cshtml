<ul class="navbar-nav">
    @if (User.Identity.IsAuthenticated)
    {
        var firstName = string.Empty;
        var lastName = string.Empty;
        if (User.Identity.IsAuthenticated)
        {
            firstName = User.Claims.FirstOrDefault(c => c.Type == "FirstName")?.Value;
            lastName = User.Claims.FirstOrDefault(c => c.Type == "LastName")?.Value;
        }
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage"><strong>@firstName<br />@lastName</strong></a>
        </li>
        <li class="nav-item">
            <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                <button type="submit" class="nav-link btn btn-link text-dark">Logout</button>
            </form>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Register">Register</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Login">Login</a>
        </li>
    }
</ul>